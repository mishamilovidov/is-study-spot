@model IEnumerable<ISStudySpot.Models.ClassInformation>

@{
    foreach (var item in Model)
    {
        ViewBag.Semester = item.SubjectCode;
    }

    ViewData["Title"] = @ViewBag.Semester;
}

<div class="jumbotron">
    <h2>@ViewData["Title"]</h2>

    <div class="row">
        <div class="col-md-12">
            @foreach (var item in Model)
            {
                ViewBag.ID = item.ClassId;
                    <p>@Html.DisplayFor(modelItem => item.ClassNoteSectionTypeDescription) - @Html.DisplayFor(modelItem => item.ClassNoteSectionTitle)</p>
            }
        </div>
    </div>
</div>

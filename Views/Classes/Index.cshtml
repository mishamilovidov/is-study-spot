@model IEnumerable<ISStudySpot.Models.Subjects>

@{
    foreach (var item in Model)
    {
        ViewBag.Semester = item.SubjectCode;
    }

    ViewData["Title"] = @ViewBag.Semester;
}

<div class="jumbotron">
    <h2>@ViewData["Title"]</h2>

    <div class="row">
        <div class="col-md-12">
            @foreach (var item in Model)
            {
                ViewBag.ID = item.ClassId;
                <!--<a asp-controller="classes" asp-action="semester" asp-route-id="@ViewBag.ID">-->
                    <p>@Html.DisplayFor(modelItem => item.SubjectCode) - @Html.DisplayFor(modelItem => item.SubjectName)</p>
                <!--</a>-->
            }
        </div>
    </div>
</div>
